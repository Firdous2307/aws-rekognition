#!/bin/bash

# Build Docker image
docker build -t athlete-rekognition .

# Tag the Docker image
docker tag athlete-rekognition:latest $ACCOUNT_ID.dkr.ecr.$AWS_DEFAULT_REGION.amazonaws.com/rekognition-repo:latest

# Push the Docker image to Amazon ECR
docker push $ACCOUNT_ID.dkr.ecr.$AWS_DEFAULT_REGION.amazonaws.com/rekognition-repo:latest
